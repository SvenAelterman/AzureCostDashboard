{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SQL_UsageAggregates",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SubscriptionId as string,",
				"          UsageStartTime as timestamp,",
				"          UsageEndTime as timestamp,",
				"          MeterName as string,",
				"          MeterCategory as string,",
				"          MeterSubCategory as string,",
				"          Unit as string,",
				"          InstanceData as string,",
				"          MeterId as string,",
				"          Quantity as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 parse(InstanceData = InstanceData,",
				"     format: 'json',",
				"     documentForm: 'singleDocument') ~> parse1"
			]
		}
	}
}